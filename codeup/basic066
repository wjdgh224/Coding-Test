import java.util.Scanner;

 public class practice {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		for(int i=0; i<3; i++) {
			int a = scanner.nextInt();
			if(a%2==0)
				System.out.println("even");
			else
				System.out.println("odd");
		}
		scanner.close();
	}
}
