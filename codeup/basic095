import java.util.Scanner;

 public class practice {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int x = scanner.nextInt(); int min = 100;
		for(int i=0; i<x; i++) {
			int y = scanner.nextInt();
			if(min>y)
				min = y;
		}
		System.out.println(min);
		scanner.close();
	}
}
