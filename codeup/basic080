import java.util.Scanner;

 public class practice {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt(); int sum = 0, i = 1;
		while(true) {
			sum += i;
			if(sum==n) {
				System.out.println(i);
				break;
			}
			else if(sum>n) {
				System.out.println(i-1);
				break;
			}
			i++;
		}
		scanner.close();
	}
}
